
<section class="content-header">
<h1>Tentukan
<small>Rangking</small></h1>
<%if (notice != nil)%>
<div class="alert alert-success">
  <%= notice %>
</div>
<% end %>
<% @url = rangkings_url %>
</section>
<section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-body">
          <% if (Rangking.all.count < Alternatif.all.count * Kriterium.all.count) %>
            <%= render 'form', rangking: @rangking %>
          <% else %>
          <div class="callout callout-info">
            <h4>Selamat!</h4>

            <p>Perangkingan sudah lengkap!</p>
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</section>
